public class Random {
  public static void main (String [1] args) {
    int a= Double.parseDouble(args[0]);
    int b= Double.parseDouble(args[1]);
    int c= (Math.random()*(b-a)+a); // Weil Math.random nur 0 und 1 ausgeben
  if b>=a //Falls identische Argumente, nichts ausgeben
    System.out.print("");
  else 
    System.out.println(c);
 }
}
